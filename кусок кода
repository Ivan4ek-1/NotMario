# Обработка прыжка
    if not is_jumping:
        if keys[pygame.K_SPACE]:  # Прыжок при нажатии пробела
            is_jumping = True
    else:
        if jump_count >= -10:
            neg = 1
            if jump_count < 0:
                neg = -1
            character_y -= (jump_count ** 2) * 0.5 * neg
            jump_count -= 1
        else:
            is_jumping = False
            jump_count = 10  # Сбросить счетчик прыжка
